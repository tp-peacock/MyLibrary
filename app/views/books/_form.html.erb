<table style="width:80%" align="center" class="table table-condensed">
	<thead>
		<tr>
			<th align="left">Position</th>
			<th align="left">Title</th>
			<th align="left">Author</th>
			<th align="center">Physical</th>
			<th align="center">E-Book</th>
			<th align="center">Read</th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	<tbody>	
		<tr>
			<td><%= f.text_field(:position) %></td>
			<td><%= f.text_field(:title) %></td>
			<td><%= f.text_field(:author) %></td>
			<td align="center"><%= f.check_box(:physical)  %></td>
			<td align="center"><%= f.check_box(:ebook) %></td>
			<td align="center"><%= f.check_box(:read) %></td>
			<td>
				<button type="submit" class= "btn btn-default btn-xs">
					Add Book
				</button>
			</td>
		</tr>
	</tbody>	
</table>